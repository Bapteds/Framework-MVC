{% extends "layout.html.twig" %}
{% block contenu %}

    {% if uneCommande %}
        {%  set idCommande = uneCommande.getId() %}
    {% endif %}
    {% block recherche %}
        {% include "recherche.html.twig" %}
    {% endblock %}
    {% if uneCommande %}
        <div class="container bg-faded">
            <p>
            <h2> Information de la commande numéro {{ uneCommande.getId() }}</h2>
        </p>
        <p> Date commande: {{ uneCommande.dateCde }}</p>
        <p> Numéro de facture: {{ uneCommande.noFacture }}</p>
        <p> ID client: {{ uneCommande.idClient }} </p>
    </div>
{% else %}

{% endif %}

{% endblock %}
