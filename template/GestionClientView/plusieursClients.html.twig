{% extends "layout.html.twig" %}
{% block contenu %}
    <p><h2> Liste des clients : {{ nombreClient }}</h2></p>
    <table>
        <thead>
            <tr>
                <th>id</th>
                <th>titre</th>
                <th>nom</th>
                <th>prenom</th>
                <th>adresse</th>
                <th>complement</th>
                <th>cp</th>
                <th>ville</th>
                <th>tel</th>
            </tr>
        </thead>
        <tbody>
            {% for client in desClients %}
            <tr>
                <td>{{client.id}}</td>
                <td>{{client.titreCli}}</td>
                <td>{{client.nomCli}}</td>
                <td>{{client.prenomCli}}</td>
                <td>{{client.adresseRue1Cli}}</td>
                <td>{{client.adresseRue2Cli}}</td>
                <td>{{client.cpCli}}</td>
                <td>{{client.villeCli}}</td>
                <td>{{client.telCli}}</td>
            </tr>
            {% endfor %}
        </tbody>
    </table>



{% endblock %}
{% block footer %}
    {{ parent() }}
    Fin de l'affichage
{% endblock %}